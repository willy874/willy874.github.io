{
  "typeAcquisition": {
    "exclude": ["dotenv", "vue3-i18n"]
  },
  "allowJS": true,
  "compilerOptions": {
    "module": "ES6",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@base/*": ["./backend/src/*"]
    }
  },
  "exclude": ["node_modules"],
  "jsx": "preserve"
}
